public class Student {
    String name;
    int rollNo;
    int marks;

    Student(String n, int r, int m) {
        name = n;
        rollNo = r;
        marks = m;
    }

    void display() {
        System.out.println(name);
        System.out.println(rollNo);
        System.out.println(marks);
    }

    public static void main(String[] args) {
        Student s1 = new Student("Parthi", 101, 85);
        s1.display();
    }
}
